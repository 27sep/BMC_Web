<main class="container-fluid">
    <div class="modal-dialog modal-dialog-centered">

    </div>
    <div class="page-content p-3">
        <div class="row">
            <div class="col-lg-3">
                <app-sidebar></app-sidebar>
            </div>
            <div class="col-lg-9">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="card">
                                    <div class="card-body d-flex w-100 align-items-center card-1 stat-card">
                                        <div>
                                            <div>PIT Quantum</div>
                                            <h4 class="mt-2">1 TON</h4>
                                        </div>
                                        <div class="ms-auto">
                                            <div class="stat-card-icon">
                                                <i class="fa-regular fa-file-lines"></i>
                                            </div>
                                            <div class="mt-2 text-center">
                                                <i class="fa-solid fa-arrow-right"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="card">
                                    <div class="card-body d-flex w-100 align-items-center card-2 stat-card">
                                        <div>
                                            <div>Empty PIT</div>
                                            <div class="d-flex align-items-end gap-2">
                                                <h4 class="mt-2">13</h4>
                                                <span class="stat-card-sub-title">Quality</span>
                                            </div>
                                        </div>
                                        <div class="ms-auto">
                                            <div class="stat-card-icon">
                                                <i class="fa-solid fa-receipt"></i>
                                            </div>
                                            <div class="mt-2 text-center">
                                                <i class="fa-solid fa-arrow-right"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="card">
                                    <div class="card-body d-flex w-100 align-items-center card-3 stat-card">
                                        <div>
                                            <div>PIT</div>
                                            <div class="d-flex align-items-end gap-2">
                                                <h4 class="mt-2">1</h4>
                                                <span class="stat-card-sub-title">Quality</span>
                                            </div>
                                        </div>
                                        <div class="ms-auto">
                                            <div class="stat-card-icon">
                                                <i class="fa-regular fa-circle-check"></i>
                                            </div>
                                            <div class="mt-2 text-center">
                                                <i class="fa-solid fa-arrow-right"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="card">
                                    <div class="card-body d-flex w-100 align-items-center card-4 stat-card">
                                        <div>
                                            <div>Assigned To Me</div>
                                            <div class="d-flex align-items-end gap-2">
                                                <h4 class="mt-2">3</h4>
                                                <span class="stat-card-sub-title">Quality</span>
                                            </div>
                                        </div>
                                        <div class="ms-auto">
                                            <div class="stat-card-icon">
                                                <i class="fa-solid fa-user-plus"></i>
                                            </div>
                                            <div class="mt-2 text-center">
                                                <i class="fa-solid fa-arrow-right"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mt-4">
                    <div class="breadcrumb">
                        <a routerLink=""><i class="fa-solid fa-home"></i></a>
                        <span class="mx-2">/</span>
                        <a routerLink="/superadmin/mcc/pit-view">MCC - {{tabName}}</a>
                    </div>
                </div>
                <div class="mt-4">
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item w-100" role="presentation">
                            <button class="nav-link active w-100 text-uppercase text-white" data-bs-toggle="tab"
                                data-bs-target="#pitview-tab-pane" type="button" role="tab"
                                aria-controls="pitview-tab-pane" aria-selected="true" (click)="showPitView()">Pit
                                View</button>
                        </li>
                        <!-- <li class="nav-item w-50" role="presentation"> -->
                        <!-- <button class="nav-link w-100 text-uppercase text-white" data-bs-toggle="tab"
                                data-bs-target="#todayview-tab-pane" type="button" role="tab"
                                aria-controls="todayview-tab-pane" aria-selected="false" (click)="showTodayTask()">Today
                                Task</button>
                        </li> -->
                    </ul>
                    <div class="tab-content" id="myTabContent">


                        <div class="tab-pane fade show active bg-secondary border border-white border-top-0 p-5"
                            id="pitview-tab-pane" role="tabpanel" aria-labelledby="pitview-tab" tabindex="0"
                            *ngIf="showPitTabView">
                            <div class="pit-view">

                                <div class="pit-item" *ngFor="let pit of allPitbyMcc.responseBody">
                                    <div class="pit-icon">
                                        <input type="text" value="{{pit.pitId}}" name="pitId" hidden>
                                        <img src="{{pit.pitStatus.activityColr}}" alt="Pit Icon"
                                            (click)="showPitmodalstatus(pit)" />
                                        <div class="pit-icon-timer">
                                            <img src="/assets/images/icons/stopwatch.png" alt="" />
                                            <span class="pit-icon-timer-count">14</span>
                                        </div>
                                    </div>
                                    <div class="pit-label" data-bs-toggle="modal"
                                    data-bs-target="#newModal">
                                        {{pit.pitName}}
                                    </div>
                                    <div height="20px"></div>
                                    <div class="pit-warning" *ngIf="pit.pitStatus.isEventNeeded == true">
                                        <img src="/assets/images/icons/warning.png" alt="Pit Warning"
                                            (click)="showPitmodalstatus(pit)" data-bs-target="#pitenzymeModal" />
                                    </div>
                                </div>

                            </div>
                        </div>

                        <app-today-task id="todayview-tab-pane" *ngIf="showTodayTaskView"></app-today-task>

                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

<div class="modal fade" id="newModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header bg-blue text-white">
                <h1 class="modal-title fs-5  fw-bold" id="exampleModalLabel">PIT - 01</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal"
                    aria-label="Close"></button>
            </div>
            <div class="modal-body px-4">
                <div class="text-center d-flex align-items-center border-bottom pb-2">
                    <div class="pit-form-header"></div>
                    <div class="pit-form-header fw-bold">Filling</div>
                    <div class="pit-form-header fw-bold">Mixing<br /><small>(Expected)</small></div>
                    <div class="pit-form-header fw-bold">1st Turn<br /><small>(Expected)</small></div>
                    <div class="pit-form-header fw-bold">2nd Turn<br /><small>(Expected)</small></div>
                </div>
                <div class="text-center d-flex align-items-center py-2 border-bottom">
                    <div class="pit-form-header text-start fw-bold">Date</div>
                    <div class="pit-form-header"><input type="date" name="" id="" class="form-control text-dark"></div>
                    <div class="pit-form-header"><input type="date" name="" id="" class="form-control text-dark"></div>
                    <div class="pit-form-header"><input type="date" name="" id="" class="form-control text-dark"></div>
                    <div class="pit-form-header"><input type="date" name="" id="" class="form-control text-dark"></div>
                </div>
                <div class="text-center d-flex align-items-center py-2 border-bottom">
                    <div class="pit-form-header text-start fw-bold">Inert</div>
                    <div class="pit-form-header">
                        <div class="input-group">
                            <input type="text" name="" id="" class="form-control text-dark">
                            <div class="input-group-text">
                                KG
                            </div>
                        </div>
                    </div>
                    <div class="pit-form-header">
                        <div class="input-group">
                            <input type="text" name="" id="" class="form-control text-dark">
                            <div class="input-group-text">
                                KG
