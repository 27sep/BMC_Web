<div class="modal" id="pitenzymeModal" tabindex="-1" aria-labelledby="pitModalLabel" aria-hidden="true"
    [style.display]=" pitmodalstatus  ? 'block':'none'">
    <div class="modal-dialog modal-lg modal-width">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="pitModalLabel">PIT QUANTUM 3 TON</h1>
            </div>
            <div class="modal-body p-0">
                <div class="border-bottom py-2">
                    <div class="row text-center">
                        <h6 class="text-danger"> <i class="fa fa-info-circle" aria-hidden="true"></i> {{activeNotification}}.</h6>
                    </div>
                </div>
                <div class="border-bottom py-2">
                   
                   <!-- Section for mix pits here -->
                   <!-- [style.display]=" pitmixstatus  ? 'block':'none'" -->
                    <div class="col-md-6 d-flex row text-center"  aria-hidden="true" >
                        <div class="col-md-3 d-flex align-items-center justify-content-center" [style.display]=" pitMixStatus  ? 'block':'none'" >Mixture of Two PITS</div>
                        <div class="col-md-3 pe-6" [style.display]=" pitMixStatus  ? 'block':'none'">
                            <p> PIT Mixed With</p>
                            <div class="d-flex" >
                                <div class="input-group date">
                                     <input type="text" class="form-control" placeholder="" *ngIf="clickedPit != undefined"  value ="{{clickedPit.pitName}}" readonly/>
                                </div>
                            </div>
                        </div>
                        <!-- <div class="col-md-1 p-0 w-10">
                            <div class="d-flex">
                                +
                            </div>
                        </div> -->


                        <div class="col-md-3 pe-3"  [style.display]=" pitMixStatus  ? 'block':'none'">
                            <div class="d-flex">
                                <div class="input-group date">
                                    <!-- <select name="" id="" class="form-control">
                                        <option value="">select Half empty pit</option>
                                    </select> -->
                                <div class="col-md-10"><label> &nbsp; PIT Mixed From </label></div>
                                <div class="select">
                                        <select name="mixThisPitwithSelected" (change)="getSelected($event)">
                                            <option value = "">--Select pit to mix up--</option>
                                            <option *ngFor="let pitmixedup of yellowPitarr" [value]="pitmixedup.pitId">
                                                {{pitmixedup.pitName}}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- <div class="grid">
                            <div class="grid__col-5">
                              
                                <div class="input-group">
                                    <label> Select the pit Mixed Up To &nbsp;</label>
                                    <div class="select">
                                        <select name="mixThisPitwithSelected" (change)="getSelected($event)">
                                            <option *ngFor="let pitmixedup of yellowPitarr" [value]="pitmixedup.pitId">
                                                {{pitmixedup.pitName}}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div> -->



                    </div> 
                      <!-- Section for mix pits end here -->



                    <div class="row text-primary mt-2">
                        <h6 class="ms-4">PIT Information</h6>
                    </div>
                </div>
                <div class="border-bottom py-2" [style.display]=" isEventNeeded  ? 'block':'none'">
                    <div class="row text-center">
                        <div class="col-md-3 d-flex align-items-center justify-content-center">Segregation</div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center pe-5">
                            <div class="toggle-button-cover">
                                <div id="switch-button" class="button r">
                                    <input class="checkbox" type="checkbox"  id="segregation" (change)="onActiveStatusChange($event,'segregation')">
                                    <div class="knobs"></div>
                                    <div class="layer"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center">Shreading</div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center pe-5">
                            <div class="toggle-button-cover">
                                <div id="switch-button" class="button r">
                                    <input class="checkbox" type="checkbox" id="shreding"
                                    (change)="onActiveStatusChange($event,'shreading')">
                                    <div class="knobs"></div>
                                    <div class="layer"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="border-bottom py-2" [style.display]=" isEventNeeded  ? 'block':'none'">
                    <div class="row text-center">
                        <div class="col-md-3 d-flex align-items-center justify-content-center">Visual Inspection</div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center pe-5">
                            <div class="toggle-button-cover">
                                <div id="switch-button" class="button r">
                                    <input class="checkbox" type="checkbox" id="visualinspection" (change)="onActiveStatusChange($event,'visuaInspection')">
                                    <div class="knobs"></div>
                                    <div class="layer"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center">Transfer to PIT</div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center pe-5">
                            <div class="toggle-button-cover">
                                <div id="switch-button" class="button r">
                                    <input class="checkbox" type="checkbox">
                                    <div class="knobs"></div>
                                    <div class="layer"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="border-bottom py-2" [style.display]=" isEventNeeded  ? 'block':'none'">
                    <div class="row text-center">
                        <!-- <div class="col-md-3 d-flex align-items-center justify-content-center">Enzyme Consumed Qty</div>
                        <div class="col-md-3 pe-5">
                            <div class="d-flex">
                                <div class="input-group">
                                    <input type="text" name="" id="" class="form-control form-control-sm"  id="mixedenzyme">
                                    <div class="input-group-text bg-light">
                                        <span>
                                            Litter
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div> -->
                        <div class="col-md-3 d-flex align-items-center justify-content-center">PIT Fillup Status</div>
                        <div class="col-md-3 d-flex align-items-center justify-content-center pe-5">
                            <div class="toggle-button-cover">
                                <div id="switch-button" class="button r">
                                    <input class="checkbox" type="checkbox" id="pitfillupstatus"
                                    (change)="onActiveStatusChange($event,'isFilledUp')">
                                    <div class="knobs"></div>
                                    <div class="layer"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row mt-3 pb-1 w-100 ms-0 px-2">
                    <div class="col-md-6 mb-3">
                        <div class="form-group d-flex">
                            <label for="" class="form-label d-flex align-items-center mb-0" style="min-width: 50%;">Enzyme Consumed Qty</label>
                            <div>
                                <div class="input-group">
                                    <input type="number" name="" id="" class="form-control"  id="mixedenzyme">
                                    <div class="input-group-text bg-light">
                                        <span>
                                            Ltr
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 mb-3">
                        <div class="form-group d-flex">
                            <label for="" class="form-label d-flex align-items-center mb-0" style="min-width: 50%;">Filled Garbage Wt.</label>
                            <div>
                                <div class="input-group">
                                    <input type="number" name="" id="" class="form-control">
                                    <div class="input-group-text bg-light" id="filledgardbagewt">
                                        <span>
                                            Ton
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 mb-3">
                        <div class="form-group d-flex">
                            <label for="" class="form-label d-flex align-items-center mb-0" style="min-width: 50%;">Cocopeat mixed Qty</label>
                            <div>
                                <div class="input-group">
                                    <input type="number" name="cocopeatMixedQtyName" id="cocopeatMixedQty" class="form-control">
                                    <div class="input-group-text bg-light">
                                        <span>
                                            Kg
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 mb-3">
                        <div class="form-group d-flex">
                            <label for="" class="form-label d-flex align-items-center mb-0" style="min-width: 50%;">Inert material mixed Qty</label>
                            <div>
                                <div class="input-group">
                                    <input type="number" name="" id="inertMaterialMxedQty" class="form-control"> 
                                    <div class="input-group-text bg-light">
                                        <span>
                                            Ton
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 mb-3">
                        <div class="form-group d-flex">
                            <label for="" class="form-label d-flex align-items-center mb-0" style="min-width: 50%;">Total compost Generated</label>
                            <div>
                                <div class="input-group">
                                    <input type="number" name="" id="totalCompostGen" class="form-control">
                                    <div class="input-group-text bg-light">
                                        <span>
                                            Kg
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer d-flex justify-content-center">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"
                    (click)="closePitActivityModalstatus()">Cancel</button>
                <button type="button" class="btn btn-primary" (click)="executeUpdateStatusByPitCurrentStatus()"  [style.display]=" isEventNeeded  ? 'block':'none'"   >Submit</button>
            </div>
        </div>
    </div>
</div>


